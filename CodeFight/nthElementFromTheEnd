int nthElementFromTheEnd(ListNode<int> * l, int n) {
    int cnt = 0,i=0;
    ListNode<int> * head = l;
    while(head!=NULL){
        head = head->next;
        cnt++;
    }

    if(n>cnt){
        return -1;
    }
    
    while(i<cnt-n){
        l = l->next;
        i++;
    }

    return l->value;
}
